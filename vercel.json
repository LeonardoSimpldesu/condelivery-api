{
    "buildCommand": "npm run vercel-build",
    "installCommand": "npm install",
    "functions": {
      "api/*.ts": {
        "memory": 1024,
        "maxDuration": 60
      }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/app.ts"
        }
    ]
}
